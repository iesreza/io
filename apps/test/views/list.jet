<table {{ param.Attribs.Render() }} >
	<tr>
		<thead>
			{{range param.Columns}}
				<th{{ if .Width > 0}} width="{{.Width}}%"{{end}}>{{.Title}}</th>
            {{end}}
		</thead>
	</tr>
	<tr>
    	<thead>
    		{{range param.Columns}}
    			<td>{{.Filter(request).Render()|raw}}</td>
            {{end}}
    	</thead>
    </tr>
</table>