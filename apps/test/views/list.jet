<table {{ param.Attribs.Render() }} >
	<thead>
	<tr>
		{{range param.Columns}}
			<th{{ if .Width > 0}} width="{{.Width}}%"{{end}}>{{.Title}}</th>
        {{end}}
	</tr>
	<tr>
    	{{range param.Columns}}
    		<td>{{.Filter(request).Render()|raw}}</td>
        {{end}}
    </tr>
    </thead>
    <tbody>
        {{range row := param.GetData()}}
                <tr>
            	{{range column := param.Columns}}
            	    <td>{{column.Processor(column.Name,row,request)|raw}}</td>
            	{{end}}
            	</tr>
        {{end}}
	</tbody>
</table>